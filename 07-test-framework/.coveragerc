[run]
# 覆蓋率配置
source = .
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    */build/*
    */dist/*

[report]
# 報告配置
precision = 2
show_missing = True
skip_covered = False
skip_empty = True

exclude_lines =
    # 排除 pragma 註釋
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
